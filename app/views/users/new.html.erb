<%=form_with model: @user do |f|%>
<div>
    <%= f.label :username %>
    <%= f.text_field :username %>
</div>
<div>
    <%= f.label :email %>
    <%= f.email_field :email %>
</div>
<div>
    <%= f.label :password %>
    <%= f.password_field :password %>
</div>
<%= submit_tag 'Create New User' %>
<%end%>